{{#if model.matured}}
<h4>{{t 'blocks.matured.title'}}</h4>
<div class="table-responsive">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th>{{t 'blocks.table.height'}}</th>
        <th>{{t 'blocks.table.blockhash'}}</th>
        <th>{{t 'blocks.table.timefound'}}</th>
        <th>{{t 'blocks.table.variance'}}</th>
        <th>{{t 'blocks.table.reward'}}</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.matured as |block|}}
        {{partial "blocks/block"}}
      {{/each}}
    </tbody>
  </table>
</div>
{{else}}
<h3>{{t 'blocks.matured.nomaturedblock'}}</h3>
{{/if}}
