{{#if model.immature}}
<h4>{{t 'blocks.immature.title'}}</h4>

<div class="table-responsive">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th>{{t 'blocks.table.height'}}</th>
        <th>{{t 'blocks.table.blockhash'}}</th>
        <th>{{t 'blocks.table.timefound'}}</th>
        <th>{{t 'blocks.table.variance'}}</th>
        <th>{{t 'blocks.table.reward'}}</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.immature as |block|}}
        {{partial "blocks/block"}}
      {{/each}}
    </tbody>
  </table>
</div>
{{else}}
<h3>{{t 'blocks.immature.noimmatureblock'}}</h3>
{{/if}}
